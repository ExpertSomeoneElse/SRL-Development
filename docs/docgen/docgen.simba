// pyinstaller .\docgen.py -F

var
  DOC_GEN_DIR = {$MACRO CURRENT_DIRECTORY};
var
  Output: String;
  Success: Boolean;
begin
  Success := RunCommandInDir(DOC_GEN_DIR, DOC_GEN_DIR + 'docgen.exe', [DOC_GEN_DIR + '../../'], Output) = 0;
  if Success then
    OpenWebPage(DOC_GEN_DIR + 'build\index.html');

  WriteLn(Output);
  if (not Success) then
    WriteLn('docgen failed!');
end.
